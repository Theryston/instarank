{
  "name": "instarank",
  "description": "Discover your competitor's most engaged post",
  "private": true,
  "scripts": {
    "build": "pnpm services:prepare:prod && next build",
    "dev": "pnpm services:prepare:dev && pnpm next",
    "start": "next start",
    "services:up": "docker-compose up -d",
    "services:stop": "docker-compose down -v",
    "database:migrate:dev": "prisma migrate dev",
    "database:migrate:prod": "prisma migrate deploy",
    "database:generate": "prisma generate",
    "services:prepare:prod": "pnpm database:generate && pnpm database:migrate:prod",
    "services:prepare:dev": "pnpm services:up && pnpm database:generate && pnpm database:migrate:dev"
  },
  "dependencies": {
    "@prisma/client": "^4.5.0",
    "date-fns": "^2.29.3",
    "next": "12.3.1",
    "next-connect": "^0.13.0",
    "pino": "^8.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/styled-components": "^5.1.25",
    "prisma": "^4.5.0",
    "typescript": "^4.7.4"
  },
  "prettier": {
    "singleQuote": true
  }
}
